
<h1>Menu</h1>

<div class="row">
  <div class="col-md-6 col-md-offset-3">
     <%= form_for(@inventory, url: menu_path) do |f| %>
    
     

      <%= f.label :name_of_item %>
      <%= f.text_field :item_name, class: 'form-control' %>

      <%= f.label :Quantity %>
      <%= f.text_field :quantity_available, class: 'form-control' %>

   

      <%= f.submit "update", class: "btn btn-primary" %>
    <% end %>
  </div>
</div>