<div class="center jumbotron">
<h1>Update</h1>
   <%= link_to "go to restaurant home", restauranthome_path, class: "btn btn-lg btn-primary" %>
    <%= link_to "home", managerhome_path, class: "btn btn-lg btn-primary" %>
<div class="row">
  <div class="col-md-6 col-md-offset-3">
     <%= form_for(@inventory, url: managerupdate_path) do |f| %>
    
     
      <%= f.label :name_of_item %>
      <%= f.text_field :item_name, class: 'form-control' %>

      <%= f.label :Quantity %>
      <%= f.text_field :quantity_available, class: 'form-control' %>

 
      <%= f.submit "update", class: "btn btn-primary" %>
    <% end %>
    <%= link_to 'Back To Inventory', {:action => 'view' } %>
  </div>
</div>
</div>
